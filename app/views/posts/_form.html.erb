<%= form_for(@post) do |f| %>
	<%= render 'shared/errors' obj: @post %>
	<%= f.label 		:title %>
	<%= f.text_field 	:title %>

	<%= f.label 		:url %>
	<%= f.text_field 	:url %>

	<%= f.label 		:description %>
	<%= f.text_area 	:description %>
	<br/>
	<%= f.submit class: "btn btn-primary"%>
<% end %>
